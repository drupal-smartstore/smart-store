{
    "name": "smartstore/smart_store",
    "description": "Drupal Commerce related projects maintained by smart store.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/drupal-smartstore",
    "authors": [
        {
            "name": "Smart Store"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
