                  "reference": "8.x-2.x"
                }
            }
        }
    ],
    "require": {
        "cweagans/composer-patches": "^1.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/adminimal_theme": "^1.7",
        "drupal/better_exposed_filters": "^6",
        "drupal/commerce": "^2.35",
        "drupal/default_content": "^2.0@alpha",
        "drupal/estore": "^2.2",
        "drupal/kart": "1.0",
        "smartstore/smart_store_admin":"1.x-dev",
        "smartstore/vitoria": "1.0.x-dev",
        "drupal/commerce_autosku": "^2.1",
        "drupal/backup_migrate": "^5.0",
        "drupal/color_field": "^3.0",
        "drupal/search_api": "^1.35",
        "drupal/facets": "^2.0",
        "drupal/fivestar": "^1.0@alpha",
        "drupal/votingapi": "^3.0@beta",
        "drupal/commerce_ajax_atc": "^1.0@RC",
        "drupal/field_group": "^3.4",
        "drupal/core_views_facets": "^2.1",
        "drupal/facets_pretty_paths": "^2.0",
        "drupal/commerce_wishlist": "^3.0@beta",
        "drupal/commerce_shipping": "^2.9",
        "drupal/simplenews": "^4.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/commerce_combine_carts": "^1.0@RC",
        "drupal/login_redirect_per_role": "^1.9",
        "drupal/password_policy": "^4.0",
        "drupal-gitlab/change_user_route": "dev-8.x-2.x@dev"
        

    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal-gitlab/change_user_route": {
                "Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-06-15/change_user_route.3.0.0.rector.patch"
            }
        }
    }
}
